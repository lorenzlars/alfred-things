				<string>var system = Application.currentApplication()
system.includeStandardAdditions = true

function writeFile(filename, content) {
  system.doShellScript(`echo '${content}' &gt; "$(pwd)/${filename}"`)
}

function readFile(filename) {
  return system.doShellScript(`cat "$(pwd)/${filename}"`)
}

function run(argv) {
  var query = argv[0]
  var data = JSON.parse(readFile('data.json'))


  let history = data.history.sort(function (a, b) {
    if (a.timestamp &gt; b.timestamp) {
      return -1
    } else {
      return 1
    }
  })

  const entry = history.find((entry) =&gt; entry.title === query)

  if (entry) {
    history[history.indexOf(entry)].timestamp = new Date().toISOString()
  } else {
    history.unshift({ title: query, timestamp: new Date().toISOString() })

    if (history.length &gt; 5) {
      history.pop()
    }
  }


  writeFile('data.json', JSON.stringify({ history }))
}</string>

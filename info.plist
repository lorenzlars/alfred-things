				<string>var system = Application.currentApplication()
system.includeStandardAdditions = true

function readFile(filename) {
  return system.doShellScript(`cat "$(pwd)/${filename}"`)
}

function run(argv) {
  var query = argv[0];
  var data = JSON.parse(readFile('data.json'))

  const history = data.history.sort(function (a, b) {
    if (a.timestamp &gt; b.timestamp) {
      return -1;
    } else {
      return 1;
    }
  })

  const items = history.map((item) =&gt; ({ title: item.title, arg: item.title }))
  const options = { items }


  if (query) {
    options.items.unshift({
      title: query,
      arg: query,
    })
  } else {
    options.items.unshift({
      title: 'Inbox',
    })
  }

  return JSON.stringify(options)
}</string>
